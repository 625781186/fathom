{
    "manifest_version": 2,
    "name": "Fathom WebExtension",
    "version": "1.0.0",
    "description": "Tools for developing Fathom rulesets",
    "icons": {
        "32": "icons/leftpad-32.png"
    },
    "browser_action": {
        "default_icon": "icons/leftpad-32.png",
        "default_title": "Freeze-Dry Corpus"
    },
    "background": {
        "scripts": ["background_scripts/index.js"]
    },
    "permissions": [
        "<all_urls>"
    ]
}
